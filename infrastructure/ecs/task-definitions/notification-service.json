{
  "family": "notification-service-task",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "notification-service",
      "image": "ghcr.io/omkardongre/notification-service:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3005
        }
      ],
      "essential": true,
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::socialhub-env-files/notification-service.env",
          "type": "s3"
        }
      ],
      "environment": [{ "name": "PORT", "value": "3006" }],
      "cpu": "256",
      "memory": "512"
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512"
}
